{
  "name": "hana_connection",
  "displayName": "HANA Connection",
  "version": "1.0.0",
  "description": "SAP HANA Cloud database connection management with connection pooling and query execution",
  "category": "Infrastructure",
  "enabled": true,
  "requiresHana": true,
  "dependencies": {
    "python": [
      "hdbcli>=2.0.0"
    ]
  },
  "structure": {
    "backend": "backend",
    "tests": "tests",
    "docs": "docs"
  },
  "api": {
    "endpoints": [],
    "provides": [
      "HANAConnection",
      "HANADataSource"
    ]
  },
  "configuration": {
    "host": "HANA_HOST environment variable",
    "port": "HANA_PORT environment variable (default: 443)",
    "user": "HANA_USER environment variable",
    "password": "HANA_PASSWORD environment variable",
    "encrypt": true,
    "sslValidateCertificate": false
  },
  "backend": {
    "type": "data_source",
    "module_path": "modules.hana_connection.backend",
    "note": "No Blueprint - this is a data source implementation"
  }
}