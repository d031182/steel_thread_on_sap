# SAP HANA Cloud Connection Configuration
# Copy this file to .env and fill in your actual values

# HANA Cloud Instance Details
HANA_HOST=your-instance-id.hana.prod-eu10.hanacloud.ondemand.com
HANA_PORT=443
HANA_USER=P2P_DEV_USER
HANA_PASSWORD=your-password-here
HANA_SCHEMA=P2P_DATA_PRODUCTS

# Server Configuration
PORT=3000
NODE_ENV=development

# Security (for production)
# ENABLE_AUTH=true
# JWT_SECRET=your-jwt-secret-here
