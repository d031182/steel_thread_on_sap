{
  "name": "sqlite_connection",
  "displayName": "SQLite Connection",
  "version": "1.0.0",
  "description": "SQLite database connection and data source management",
  "category": "Infrastructure",
  "enabled": true,
  "requiresHana": false,
  "structure": {
    "backend": "backend/"
  },
  "backend": {
    "type": "data_source",
    "module_path": "modules.sqlite_connection.backend",
    "note": "No Blueprint - this is a data source implementation, not an API module"
  },
  "provides": {
    "interfaces": ["DataSource"],
    "services": ["SQLiteDataSource"]
  },
  "dependencies": {
    "core": ["DataSource interface"],
    "modules": ["data_products (for SQLiteDataProductsService)"]
  }
}
