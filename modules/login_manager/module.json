{
  "name": "login_manager",
  "displayName": "Login Manager",
  "version": "1.0.0",
  "description": "User authentication and session management with auto-login support",
  "category": "Infrastructure",
  "enabled": true,
  "dependencies": {
    "python": []
  },
  "structure": {
    "backend": "backend/",
    "frontend": "frontend/",
    "tests": "tests/",
    "docs": "docs/"
  },
  "backend": {
    "blueprint": "login_manager_api",
    "module_path": "modules.login_manager.backend"
  },
  "api": {
    "endpoints": [
      "POST /api/login-manager/login",
      "POST /api/login-manager/logout",
      "GET /api/login-manager/current-user"
    ],
    "provides": [
      "AuthenticationService",
      "get_current_user"
    ]
  },
  "configuration": {
    "default_user": "HANA_DP_USER",
    "session_timeout": 3600,
    "auto_login": true
  }
}